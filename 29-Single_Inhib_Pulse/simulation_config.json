{
  "manifest": {
    "$OUTPUT_DIR": "$BASE_DIR\\output", 
    "$BASE_DIR": "${configdir}"
  }, 
  "target_simulator": "NEURON", 
  "run": {
    "dL": 20.0, 
    "nsteps_block": 5000, 
    "spike_threshold": -15.0, 
    "tstart": 0.0, 
    "tstop": 30.0, 
    "dt": 0.01
  }, 
  "conditions": {
    "celsius": 34.0, 
    "v_init": -66.5
  }, 
  "inputs": {
    "extnet_spikes": {
      "input_type": "spikes", 
      "node_set": "extnet", 
      "module": "sonata", 
      "input_file": "$BASE_DIR\\input\\Splay.h5"
    }, 
    "file_current_clamp": {
      "input_type": "current_clamp", 
      "node_set": "all", 
      "module": "FileIClamp", 
      "input_file": "$BASE_DIR\\input\\amps.h5"
    }, 
    "inhib_spikes": {
      "input_type": "spikes", 
      "node_set": "inhib", 
      "module": "sonata", 
      "input_file": "$BASE_DIR\\input\\Inhib.h5"
    }
  }, 
  "output": {
    "spikes_file": "spikes.h5", 
    "log_file": "log.txt", 
    "output_dir": "$OUTPUT_DIR"
  }, 
  "reports": {
    "v_report": {
      "cells": "all", 
      "sections": "soma", 
      "module": "membrane_report", 
      "variable_name": "v"
    }
  }, 
  "node_sets_file": "$BASE_DIR\\node_sets.json", 
  "network": "$BASE_DIR\\circuit_config.json"
}